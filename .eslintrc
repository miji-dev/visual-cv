{
    "rules": {
        "indent": [
            2,
            4
        ],
        "quotes": [
            2,
            "double"
        ],
        "semi": [
            2,
            "always"
        ],
        "yoda": [
            2,
            "never"
        ],
        "consistent-return": 2,
        "curly": 2,
        "default-case": 2,
        "guard-for-in": 1,
        "no-alert": 2,
        "no-eval": 2,
        "no-extend-native": 1,
        "no-loop-func": 2,
        "no-param-reassign": 2,
        "no-self-compare": 2,
        "no-void": 2,
        "no-with": 2,
        "vars-on-top": 2
    },
    "env": {
        "es6": true,
        "browser": true
    },
    "extends": "eslint:recommended"
}